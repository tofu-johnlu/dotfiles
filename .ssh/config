# Personal github account
Host github.com
  HostName github.com
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

# Exwzd github account
Host exwzd.github.com
  HostName github.com
  IdentityFile ~/.ssh/id_exwzd
  IdentitiesOnly yes

# Exwzd gitlab account
Host exwzd.gitlab.com
  HostName gitlab.com
  IdentityFile ~/.ssh/id_exwzd
  IdentitiesOnly yes
